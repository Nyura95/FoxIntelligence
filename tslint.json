{
  "defaultSeverity": "error",
  "extends": [],
  "jsRules": {},
  "project": "./tsconfig.json",
  "rules": {
    "adjacent-overload-signatures": true,
    "variable-name": [
      true,
      "allow-pascal-case"
    ],
    "semicolon": [
      true,
      "always"
    ],
    "no-boolean-literal-compare": false,
    "interface-name": [
      true,
      "always-prefix"
    ],
    "import-name": false,
    "no-any": true,
    "no-console": [
      true,
      "log",
      "error",
      "trace",
      "info"
    ]
  },
  "rulesDirectory": []
}